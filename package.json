{
  "name": "webpack-stater",
  "version": "1.0.0",
  "description": "A template for developing your frontend and backend in one project using webpack-dev-server and nodemon",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently --kill-others \"webpack-dev-server\" \"nodemon server.js\"",
    "build": "webpack",
    "start": "node server.js",
